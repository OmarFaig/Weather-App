<Window x:Class="WeatherAppGUI.MainWindow"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:WeatherAppGUI"
        x:DataType="local:MainWindowViewModel"
        Title="Weather App" Width="800" Height="600">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock Text="Weather Data" FontSize="20" FontWeight="Bold" Margin="0,0,0,10"/>
        <TextBlock Text="{Binding Temperature}" FontSize="16"/>
        <TextBlock Text="{Binding Description}" FontSize="16"/>
        <TextBlock Text="{Binding Humidity}" FontSize="16"/>
        <TextBlock Text="{Binding WindSpeed}" FontSize="16"/>
    </StackPanel>
</Window>
